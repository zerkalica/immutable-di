{
  "name": "immutable-di",
  "version": "1.1.9",
  "description": "Simple, promise-based dependency injection container with some state handling functions (for facebook flux-like state-management)",
  "scripts": {
    "build": "babel src --out-dir=dist",
    "test": "mocha --compilers js:babel-core/register --recursive src/**/__tests__/*.js",
    "dev": "mocha --compilers js:babel-core/register --recursive --watch src/**/__tests__/*.js",
    "dev.examples": "webpack-dev-server --config webpack.config.js --hot --inline"
  },
  "author": "Stefan Zerkalica <zerkalica@gmail.com>",
  "license": "MIT",
  "main": "immutable-di.js",
  "repository": {
    "type": "git",
    "url": "http://github.com/zerkalica/immutable-di.git"
  },
  "dependencies": {
    "babel-runtime": "^5.5.8",
    "core-js": "^0.9.18",
    "debug": "^2.2.0"
  },
  "devDependencies": {
    "babel": "^5.5.8",
    "babel-core": "^5.5.8",
    "babel-eslint": "^3.1.16",
    "babel-loader": "^5.1.4",
    "babel-plugin-closure-elimination": "0.0.1",
    "babel-plugin-espower": "^1.0.0",
    "eslint": "^0.23.0",
    "html-webpack-plugin": "^1.5.2",
    "mocha": "^2.2.5",
    "power-assert": "^0.11.0",
    "proxyquire": "^1.5.0",
    "react": "^0.13.3",
    "sinon": "^1.15.3",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0"
  }
}

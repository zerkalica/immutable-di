{
  "name": "immutable-di",
  "version": "1.1.9",
  "description": "Simple, promise-based dependency injection container with some state handling functions (for facebook flux-like state-management)",
  "scripts": {
    "build": "babel src --out-dir=dist",
    "test": "mocha --compilers js:babel/register --recursive **/__tests/*",
    "dev": "mocha --compilers js:babel/register --recursive --watch **/__tests/*",
    "dev.examples": "webpack-dev-server --config webpack.config.js --hot --inline"
  },
  "author": "Stefan Zerkalica <zerkalica@gmail.com>",
  "license": "MIT",
  "main": "immutable-di.js",
  "repository": {
    "type": "git",
    "url": "http://github.com/zerkalica/immutable-di.git"
  },
  "dependencies": {
    "babel-runtime": "^5.4.7",
    "core-js": "^0.9.13",
    "debug": "^2.2.0"
  },
  "devDependencies": {
    "autobind-decorator": "^1.2.0",
    "babel": "^5.4.7",
    "babel-core": "^5.4.7",
    "babel-eslint": "^3.1.9",
    "babel-loader": "^5.1.3",
    "babel-plugin-closure-elimination": "0.0.1",
    "chai": "^2.3.0",
    "chai-as-promised": "^5.0.0",
    "deepmerge": "^0.2.10",
    "eslint": "^0.22.1",
    "html-webpack-plugin": "^1.4.0",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "proxyquire": "^1.5.0",
    "react": "^0.13.3",
    "sinon": "^1.14.1",
    "sinon-chai": "^2.7.0",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  }
}
